   <kon:Shape rdf:about="http://kerameikos.org/id/belly_amphora">
        <skos:prefLabel xml:lang="en">Belly Amphora</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">A belly amphora has a continuous, smooth curve from lip to foot. These amphorae have been classified as Types A, B, and C, based on differences in the shape of lips, feet, and handles.</skos:definition>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
        <skos:altLabel xml:lang="en">Continuous-Curve Amphora</skos:altLabel>
        <skos:altLabel xml:lang="en">One-Piece Amphora</skos:altLabel>
        <skos:exactMatch rdf:resource="http://collection.britishmuseum.org/id/thesauri/x105151"/>
        <skos:exactMatch rdf:resource="http://vocab.getty.edu/aat/300198852"/>
        <skos:broader rdf:resource="http://kerameikos.org/id/amphora"/>
        <dcterms:source rdf:resource="http://www.worldcat.org/oclc/38179875"/>
        <rdfs:seeAlso rdf:resource="http://www.perseus.tufts.edu/hopper/text?doc=Perseus:text:1999.04.0004:id=belly-amphora"/>
        <rdfs:seeAlso rdf:resource="http://www.beazley.ox.ac.uk/tools/pottery/shapes/belly.htm"/>
        <skos:changeNote rdf:resource="http://kerameikos.org/id/belly_amphora#provenance"/>
        <dcterms:source rdf:resource="https://zenon.dainst.org/Record/000926820"/>
        <skos:prefLabel xml:lang="fr">amphore à panse</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Bauchamphora</skos:prefLabel>
        <skos:prefLabel xml:lang="el">Πλατύσωμος αμφορέας</skos:prefLabel>
        <skos:prefLabel xml:lang="it">anfora a profilo continuo</skos:prefLabel>
        <skos:prefLabel xml:lang="pl">amfora wybrzuszona</skos:prefLabel>
        <skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q11077005"/>
        <skos:definition xml:lang="de">Die Gefäßform der Bauchamphore ist von der Lippe bis zum Fuß durchgehend und fließend. Diese Amphoren werden eingeteilt in Typ A, B, C auf Grund von unterschiedlichen Lippen-, Fuß-, und Henkelformen.</skos:definition>
    </kon:Shape>
