        <ontolex:otherForm>
					       <ontolex:Form>
						          <ontolex:writtenRep xml:lang="en">Phormiskoi</ontolex:writtenRep>
						          <lexinfo:number rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#plural"/>
					       </ontolex:Form>
