        <skos:prefLabel xml:lang="en">Phormiskos</skos:prefLabel>
        <ontolex:otherForm>
					       <ontolex:Form>
						          <ontolex:writtenRep xml:lang="en">Phormiskoi</ontolex:writtenRep>
