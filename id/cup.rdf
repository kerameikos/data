   <kon:Shape rdf:about="http://kerameikos.org/id/cup">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
        <skos:prefLabel xml:lang="en">Cup</skos:prefLabel>
        <skos:definition xml:lang="en">A general term for a two-handled container for drinking. See kylix and skyphos.</skos:definition>
        <skos:exactMatch rdf:resource="https://www.britishmuseum.org/collection/term/x6288"/>
        <skos:changeNote rdf:resource="http://kerameikos.org/id/cup#provenance"/>
        <skos:prefLabel xml:lang="de">Schale</skos:prefLabel>
        <skos:definition xml:lang="de">Ein allgemeiner Begriff für ein zweihenkeliges Gefäß zum Trinken. Siehe Kylix, Skyphos.</skos:definition>
        <skos:prefLabel xml:lang="el">κύπελλο</skos:prefLabel>
        <skos:definition xml:lang="el">Γενικός όρος για αγγείο πόσεως με δύο λαβές, Βλ. κύλικα και σκύφος.</skos:definition>
        <skos:definition xml:lang="he">מונח כללי לכלי שתייה בעל שתי ידיות. ראה קיליקס וסקיפוס (kylix and skyphos).</skos:definition>
    </kon:Shape>
