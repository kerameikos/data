        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
        <skos:prefLabel xml:lang="en">Chalice</skos:prefLabel>
        <skos:definition xml:lang="en">The chalice is a cup with a conical foot, a deep wall, and horizontal handles at base of the wall.</skos:definition>
        <dcterms:source rdf:resource="https://zenon.dainst.org/Record/000926820"/>
        <skos:prefLabel xml:lang="de">Kalyx</skos:prefLabel>
        <skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q1722790"/>
        <skos:exactMatch rdf:resource="https://www.britishmuseum.org/collection/term/x5880"/>
        <skos:changeNote rdf:resource="http://kerameikos.org/id/chalice#provenance"/>
        <skos:definition xml:lang="de">Der Kelch ist eine schale mit konischem Fuß, tiefen Wänden und horizontalen Henkeln an der Basis der Wand.</skos:definition>
        <skos:prefLabel xml:lang="el">χιακή κύλικα</skos:prefLabel>
        <skos:definition xml:lang="el">Η χιακή κύλικα είναι ένα αγγείο πόσης με ένα κωνικό πόδι, βαθιά τοιχώματα και οριζόντιες λαβές στη βάση των τοιχωμάτων.</skos:definition>
        <skos:definition xml:lang="he">הגביע הינו כוס בעל רגל/תחית חרוטית, קיר עמוק, וידיות אופקיות בבסיס הקיר.</skos:definition>
    </kon:Shape>
   
   
