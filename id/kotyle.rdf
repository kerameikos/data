   <kon:Shape rdf:about="https://kerameikos.org/id/kotyle">
        <skos:prefLabel xml:lang="en">Kotyle</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">Used as an alternative for skyphos, the term kotyle refers to a deep cup with two horizontal handles that lacks an offset lip.</skos:definition>
        <skos:inScheme rdf:resource="https://kerameikos.org/id/"/>
