        <skos:prefLabel xml:lang="en">Epinetron</skos:prefLabel>
        <skos:altLabel xml:lang="en">Onos</skos:altLabel>
        <skos:definition xml:lang="en">The term refers to a long, semi-circular cover for the knee and thigh, over which wool was drawn to remove dirt. It is wider at the open end where the thigh is thicker and closed at the narrow end to fit over the knee cap. The top of the epinetron is covered with an incised scale pattern, which produced the necessary roughness when the wool was rubbed against it. The shape is uncommon in black-figure.</skos:definition>
        <dcterms:source rdf:resource="https://zenon.dainst.org/Record/000926820"/>
        <skos:prefLabel xml:lang="de">Epinetron</skos:prefLabel>
        <skos:prefLabel xml:lang="cs">Epinétron</skos:prefLabel>
        <skos:prefLabel xml:lang="el">Επίνητρον</skos:prefLabel>
        <skos:prefLabel xml:lang="es">Epinetron</skos:prefLabel>
        <skos:prefLabel xml:lang="fr">épinétron</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Epinetron</skos:prefLabel>
        <skos:prefLabel xml:lang="pt">epinetrão</skos:prefLabel>
        <skos:prefLabel xml:lang="ru">Эпинетрон</skos:prefLabel>
        <skos:prefLabel xml:lang="uk">Епінетрон</skos:prefLabel>
        <skos:prefLabel xml:lang="nl">Epinetron</skos:prefLabel>
